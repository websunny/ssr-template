
  {
    "compilerOptions": {
      "target": "ES5",
      "module": "commonjs",
      "noEmitOnError": true,
      "noImplicitAny": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,  /* Allow default imports from modules with no default export. This does not affect code emit, just typechecking. */
      "esModuleInterop": true
    },
    "exclude": [
      "dist",
      "node_modules"
    ]
  }

  